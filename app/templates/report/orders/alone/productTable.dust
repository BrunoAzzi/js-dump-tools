<table class='table table-bordered'>
    <thead>
        <tr>
            <th>#</th>
            <th>Product Id</th>
            <th>SKU</th>
            <th>Price</th>
            <th>Quantity</th>
        </tr>
    </thead>
    <tbody>
        {#values}<tr>
            <th scope='row'>#</th>
            <td>{pid}</td>
            <td>{sku}</td>
            <td>{price|formatCurrencyValue}</td>
            <td>{quantity|formatIntegerValue}</td>
        </tr>{/values}
    </tbody>
</table>
